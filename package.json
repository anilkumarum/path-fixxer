{
	"name": "path-fixxer",
	"version": "0.1.2",
	"description": "Npm package path fixer for chrome extensions",
	"repository": "anilkumarum/path-fixxer",
	"license": "MIT",
	"author": {
		"name": "Anil Kumar"
	},
	"main": "main.js",
	"type": "module",
	"exports": "./main.js",
	"engines": {
		"node": ">=16.14"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"find": "node --experimental-import-meta-resolve main.js --replace $PKG"
	},
	"keywords": [
		"chrome",
		"extension",
		"path",
		"file",
		"filepath"
	],
	"dependencies": {
		"es-module-lexer": "^1.1.0",
		"magic-string": "^0.26.7"
	}
}
